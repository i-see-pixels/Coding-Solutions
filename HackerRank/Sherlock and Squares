#include <bits/stdc++.h>
#include <cmath>

using namespace std;

// Complete the squares function below.
int squares(int a, int b) {
    int count = 0;
    count = (int)(floor(sqrt(b)) - ceil(sqrt(a)) + 1);
    return count;
}

int main()
{
    int t;
    cin>>t;
    while(t--){
        int a, b;
        cin>>a>>b;
        cout<<squares(a, b)<<endl;
    }

    return 0;
}
